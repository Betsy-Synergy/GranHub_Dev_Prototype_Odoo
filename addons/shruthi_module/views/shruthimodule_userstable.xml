<?xml version="1.0" encoding="utf-8"?>
<odoo>
<act_window
id="shruthimodule_userstable_action"
name="Users Data"
res_model="shruthimodule.userstable" />

<menuitem
 id="shruthimodule_userstable_menu"
 name="SHRUTHI Module" 
 action="shruthimodule_userstable_action"
 parent=""
 sequence="5" />

<record id ="shruthimodule_userstable_view_form" model="ir.ui.view">
   <field name="name">Users Data Form</field>
   <field name="model">shruthimodule.userstable</field>
   <field name="arch" type="xml">
         <form>
 	     <group>
	        <field name="name"/>	
	     </group>
	 </form>
   </field>
</record>

<record id="shruthimodule_userstable_view_tree" model="ir.ui.view">
  <field name="name">Users Data List</field>
  <field name="model">shruthimodule.userstable</field>
  <field name="arch" type="xml">
    <tree>
      <field name="name"/>
    </tree>
    </field>
</record>	
	

</odoo>
